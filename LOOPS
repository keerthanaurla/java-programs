int n,r;
int esum=0;
int osum=0;
System.out.println("Enter a number:");
n=Integer.parseInt(br.readLine());
while(n>0)
{
 r=n%10;
 if(r%2==0) 
   esum+=r;
 else
   osum+=r;
 n/=10;
}
System.out.println(Math.abs(esum-osum));
}
