//program which takes a string and display no of vowels in it
/*import java.io.*;
class Methods
{
 public static void main(String args[])throws Exception
 {
  BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
  String input;
  int v=0,c=0;
  input=br.readLine();
